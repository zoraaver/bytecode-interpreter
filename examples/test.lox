class Car {
    print(a) {
        while (a > 0) {
            print("Size is " + this.size + " and make is " + this.make);
            a = a - 1;
        }
    }

    drive() {
        print("Drive!");
    }

    init(size, make) {
        this.size = size;
        this.make = make;
    }
}

var car = Car("5", "ferrari");

car.print(3);
car.drive();